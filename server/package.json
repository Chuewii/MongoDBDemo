{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --ignore \"src/swagger\" --exec \"npm run swagger && npm run start:prod\"",
    "start:prod": "npm run build && node dist/server.js",
    "swagger": "tsoa spec-and-routes",
    "build": "rimraf ./dist && tsc",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "api-test": "ts-mocha src/**/*.spec.ts",
    "test": "start-test start http://127.0.0.1:5000 api-test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^5.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "assert": "^2.0.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.1",
    "start-server-and-test": "^2.0.0",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  }
}
